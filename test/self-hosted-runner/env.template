# URL points to your repo. Example: "https://github.com/foobar/nri-resmgr"
GH_RUNNER_URL=''

# Usually there is no need to tweak this.
GH_RUNNER_NAME="runner"

# You need to create github.com App and set the information here
GH_APP_ID=""
GH_APP_PRIVATE_KEY_FILE=""

# The repo is in format "{owner}/{repo}". Example: "foobar/nri-resmgr"
GH_REPO=""

VM_CPUS='8'
VM_MEMORY='32768'
VM_DISK_SIZE='30GB'

PROXY=""
HTTP_PROXY="http://${PROXY}"
HTTPS_PROXY="http://${PROXY}"
NO_PROXY="localhost,127.0.0.1,192.168.76.0/24,10.96.0.0/12"
http_proxy="$HTTP_PROXY"
https_proxy="$HTTPS_PROXY"
no_proxy="$NO_PROXY"

# Optional DNS settings
DNS_SEARCH_DOMAIN=""
DNS_NAMESERVER=""

# Optional setting for cached repository packages.
# Example: http://example.com/pub/mirrors/ubuntu
REPO_PATH_CACHED_PACKAGES=""

# Assume containerd sources are found in <this-repo-root-dir>/../containerd
# The value here is relative path from test/self-hosted-runner/runner/ubuntu-22.04
CONTAINERD_SRC=../../../../../containerd/

# If you have your own Docker registry (hostname:port)
DOCKER_REGISTRY=""
